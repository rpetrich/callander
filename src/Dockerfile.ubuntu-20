FROM ubuntu:20.04
RUN apt-get update && DEBIAN_FRONTEND=noninteractive apt-get install ubuntu-dbgsym-keyring
COPY debug.list.ubuntu /etc/apt/sources.list.d/debug.list
RUN apt-get update
RUN DEBIAN_FRONTEND=noninteractive apt-get install -y gdb binutils less moreutils expect git python3 ruby bc bzip2 curl gcc busybox file gettext bsdmainutils bind9-host systemd net-tools jq psmisc kmod landscape-common login lsb-release util-linux
COPY callander callander.debug /bin/
